{
  "manifest_version": 3,
  "name": "CursorMate",
  "version": "1.0",
  "description": "Show AI options when selecting text",
  "permissions": [
    "activeTab",
    "scripting",
    "clipboardWrite",
    "storage"
  ],
  "background": {
    "service_worker": "src/background/background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "src/ui/popup/popup.html"
  },
  "options_page": "src/ui/settings/settings.html",
  "icons": {
    "16": "assets/icons/icon16.png",
    "48": "assets/icons/icon48.png",
    "128": "assets/icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "js": [
        "assets/libs/marked.min.js",
        "src/content/content.js"
      ],
      "css": [
        "assets/styles/content.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/icons/*.png",
        "src/ui/popup/popup.html",
        "src/ui/main-ui/ui.html",
        "src/api/model.js",
        "src/content/components/*.js",
        "src/utils/constants.js",
        "src/utils/storage.js",
        "src/utils/actionHandler.js",
        "src/utils/config.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "key": ""
}